    "@types/date-fns": "^2.6.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.8.1",
    "@types/ramda": "^0.30.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-redux": "^7.1.34",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.7.7",
    "decoders": "^2.5.0",
    "ramda": "^0.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.27.0",
    "react-scripts": "^5.0.1",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.9",
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
