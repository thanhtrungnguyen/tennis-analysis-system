    "eslint-webpack-plugin": "^4.2.0",
    "globals": "^15.11.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.3.3",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  },
  "pre-commit": [
    "prettier:check",
    "lint"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "clearMocks": true,
    "collectCoverageFrom": [
      "!<rootDir>/src/index.tsx",
      "!<rootDir>/src/helpers/testsHelpers.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
